{% extends "base.jinja" %}
{% block title %}{{ user.username }}{% endblock %}
{% block content %}
{% if user %}
<h1>{{ user.username }}</h1>
<h1>{{ user.created_at }}</h1>
<h1>{{ user.location }}</h1>
<h1>{{ user.about }}</h1>
<h1>{{ user.status }}</h1>
{% else %}
<h1>User not found</h1>
{% endif %}
{% endblock %}
